{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [

      { "src": "/api/(.*)", "dest": "backend/index.js" },
  

      { "handle": "filesystem" },
  
 
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  